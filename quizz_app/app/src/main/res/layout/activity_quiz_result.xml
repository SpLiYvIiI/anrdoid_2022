<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.QuizResultActivity">

	<TextView
			android:layout_width="402dp"
			android:layout_height="107dp" android:id="@+id/quizResult"
			app:layout_constraintTop_toTopOf="parent"
			android:textAlignment="center"
			app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.376"
			android:textSize="36sp" android:gravity="center_horizontal"/>
	<Button
			android:text="Retake"
			android:layout_width="150dp"
			android:layout_height="74dp" android:id="@+id/retakeQuiz"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"
			app:layout_constraintVertical_bias="0.576" android:textSize="24sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>